  <build>
    <defaultGoal>install</defaultGoal>
    <sourceDirectory>../modules/console/gui/war/src/main/</sourceDirectory>
    <outputDirectory>../modules/console/gui/war/src/main/webapp/WEB-INF/classes</outputDirectory>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-clean-plugin</artifactId>
        <version>${maven-clean-plugin.version}</version>
        <configuration>
          <filesets>
            <fileset>
              <directory>${basedir}</directory>
              <includes>
                <include>**/*.bak</include>
                <include>**/Thumbs.db</include>
                <include>test.log</include>
                <include>test.log.*</include>
                <include>${project.artifactId}.log</include>
                <include>${project.artifactId}.log.*</include>
              </includes>
              <excludes>
                <exclude>build.log</exclude>
              </excludes>
              <followSymlinks>false</followSymlinks>
            </fileset>
          </filesets>
        </configuration>
        <executions>
          <execution>
            <id>clean-backup</id>
            <phase>process-sources</phase>
            <goals>
              <goal>clean</goal>
            </goals>
            <configuration>
              <excludeDefaultDirectories>true</excludeDefaultDirectories>
              <filesets>
                <fileset>
                  <includes>
                    <include>**/*.bak</include>
                  </includes>
                </fileset>
              </filesets>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>${maven-compiler-plugin.version}</version>
        <configuration>
          <encoding>${project.build.sourceEncoding}</encoding>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <!-- add by xinpeng 20090630-->
      <plugin>
        <artifactId>maven-resources-plugin</artifactId>
        <executions>
          <execution>
            <id>copy-resources</id>
            <!-- here the phase you need -->
            <phase>validate</phase>
            <goals>
              <goal>copy-resources</goal>
            </goals>
            <configuration>
              <outputDirectory>../modules/console/gui/war/src/main/webapp/WEB-INF/classes</outputDirectory>
              <resources>          
                <resource>
                  <directory>../modules/console/gui/war/src/main/resources</directory>
                  <filtering>true</filtering>
                </resource>
              </resources>              
            </configuration>            
          </execution>
        </executions>
      </plugin>
			<!--end by xinpeng-->
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-install-plugin</artifactId>
        <version>${maven-install-plugin.version}</version>
